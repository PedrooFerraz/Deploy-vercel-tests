{
    "version": 2,
    "builds": [
      {
        "src": "server-json/server.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["db.json"]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server-json/server.js"
      }
    ]
  }